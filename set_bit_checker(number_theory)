def printbinary(num):
    for i in range(10,-1,-1):
        print((num >> i) & 1,end='')
    print()
def is_bit_set(num,pos):
    if num & (1 >> pos) != 0:
        return True
    else:
        return False

if __name__ == "__main__":
    x = 9
    printbinary(x)
    i = 1
    if is_bit_set(x,i):
        print("Bit is set")
    else:
        print("bit is not set")
